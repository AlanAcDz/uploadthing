{
  "name": "@uploadthing/svelte",
  "version": "5.5.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "build:styles": "tailwindcss -i ./src/lib/app.css -o ./dist/app.css",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && npm run build:styles && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint \"**/*.{ts,svelte}\"",
    "prepare": "svelte-kit sync",
    "clean": "git clean -xdf dist .svelte-kit node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./app": {
      "default": "./dist/app.css"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0",
    "uploadthing": "^5.0.0"
  },
  "dependencies": {
    "@uploadthing/shared": "^5.2.1",
    "attr-accept": "^2.2.2",
    "file-selector": "^0.6.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.24.1",
    "@sveltejs/package": "^2.2.2",
    "autoprefixer": "10.4.14",
    "postcss": "8.4.22",
    "postcss-load-config": "^4.0.1",
    "publint": "^0.1.9",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.1.6",
    "uploadthing": "5.5.3",
    "vite": "^4.4.9"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "eslintConfig": {
    "root": true,
    "extends": [
      "@uploadthing/eslint-config/base",
      "@uploadthing/eslint-config/svelte"
    ]
  }
}
